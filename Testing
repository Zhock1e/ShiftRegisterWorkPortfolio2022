const int DataPin = 4; // SER Pin
const int ClockPin = 5; // RCLK9 Pin
const int LatchPin = 6; // SRCLK Pin

//Code without shift register
  //int pin2 = 2;
  //int pin3 = 3;
  //int pin4 = 4;
  //int pin5 = 5;
  //int pin6 = 6;
  //int pin7 = 7;
  //int timer = 100;

void setup() {
pinMode(DataPin, OUTPUT);
pinMode(ClockPin, OUTPUT);
pinMode(LatchPin, OUTPUT);
 
 didgitalWrite(ClockPin, LOW);
  shiftOut(DataPin,LatchPin,MSBFIRST,B10101010);
  didgitalWrite(ClockPin, HIGH);
 
 
//Code without shift register
  //  pinMode(pin2, OUTPUT);
  //  pinMode(pin3, OUTPUT);
  //  pinMode(pin4, OUTPUT);
  //  pinMode(pin5, OUTPUT);
  //  pinMode(pin6, OUTPUT);
  //  pinMode(pin7, OUTPUT);
}

void loop() {

}
